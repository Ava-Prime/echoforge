{
  "name": "@org/config",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "dependencies": {
    "zod": "^3.23.8",
    "dotenv": "^16.4.5"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --sourcemap --format cjs,esm --out-dir dist",
    "test": "vitest run",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint .",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "typescript": "^5.4.0",
    "rimraf": "^5.0.0",
    "tsup": "^8.0.2",
    "vitest": "^3.2.4"
  }
}
